{
  "name": "Calm",
  "description": "Calm.com for your Desktop",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "version": "1.0.0",
  "offline_enabled": false,
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": [
      "storage",
      "webview",
      "http://calm.com/"
  ],
  "icons": {
    "16": "assets/icon-128x128.png",
    "128": "assets/icon-128x128.png"
  }
}
